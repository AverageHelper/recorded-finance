{
	"name": "accountable-server",
	"version": "0.6.0",
	"scripts": {
		"start": "./node_modules/.bin/nodemon --watch ./dist ./dist/server.js",
		"prebuild": "rm -rf ./dist && npm run export-version",
		"build": "npm run lint && ./node_modules/.bin/rollup --config rollup.config.ts --configPlugin typescript",
		"lint": "npm run export-version && npm run lintonly",
		"lint:fix": "npm run export-version && npm run lintonly -- --fix",
		"lintonly": "./node_modules/.bin/eslint . --ext .js,.ts",
		"test": "NODE_OPTIONS='--experimental-vm-modules' ./node_modules/.bin/jest --forceExit",
		"test:watch": "npm run test -- --watch",
		"export-version": "./node_modules/.bin/genversion ./version.ts -esd"
	},
	"type": "module",
	"main": "./dist/server.js",
	"dependencies": {
		"@averagehelper/job-queue": "1.0.1",
		"atob-lite": "2.0.0",
		"bcrypt": "5.0.1",
		"btoa-lite": "1.0.0",
		"cookie-session": "2.0.0",
		"cors": "2.8.5",
		"dotenv": "16.0.1",
		"express": "4.18.1",
		"express-ws": "5.0.2",
		"fast-folder-size": "1.7.0",
		"helmet": "5.1.1",
		"jsonwebtoken": "8.5.1",
		"lowdb": "3.0.0",
		"multer": "1.4.5-lts.1",
		"n-digit-token": "2.1.0",
		"rate-limiter-flexible": "2.3.6",
		"source-map-support": "0.5.21",
		"stream-meter": "1.0.4",
		"superstruct": "0.15.5",
		"tsscmp": "1.0.6",
		"uuid": "8.3.2"
	},
	"optionalDependencies": {
		"bufferutil": "4.0.5",
		"utf-8-validate": "5.0.7"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "22.0.2",
		"@rollup/plugin-json": "4.1.0",
		"@rollup/plugin-node-resolve": "13.3.0",
		"@rollup/plugin-typescript": "8.4.0",
		"@types/atob-lite": "2.0.0",
		"@types/bcrypt": "5.0.0",
		"@types/btoa-lite": "1.0.0",
		"@types/cookie-session": "2.0.44",
		"@types/cors": "2.8.12",
		"@types/csurf": "1.11.2",
		"@types/express": "4.17.13",
		"@types/express-ws": "3.0.1",
		"@types/jest": "28.1.7",
		"@types/jsonwebtoken": "8.5.8",
		"@types/multer": "1.4.7",
		"@types/source-map-support": "0.5.4",
		"@types/stream-meter": "0.0.22",
		"@types/supertest": "2.0.12",
		"@types/tsscmp": "1.0.0",
		"@types/uuid": "8.3.4",
		"@types/ws": "8.5.3",
		"@typescript-eslint/eslint-plugin": "5.36.1",
		"@typescript-eslint/parser": "5.36.1",
		"eslint": "8.23.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-strictest": "0.4.0",
		"eslint-plugin-deprecation": "1.3.2",
		"eslint-plugin-jest": "27.0.1",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-promise": "6.0.1",
		"eslint-plugin-unicorn": "43.0.2",
		"genversion": "3.1.1",
		"jest": "28.1.3",
		"jest-environment-node": "28.1.3",
		"jest-extended": "1.2.1",
		"nodemon": "2.0.19",
		"prettier": "2.7.1",
		"rollup": "2.78.1",
		"rollup-plugin-analyzer": "4.0.0",
		"rollup-plugin-terser": "7.0.2",
		"rollup-plugin-visualizer": "5.8.0",
		"supertest": "6.2.4",
		"ts-jest": "28.0.8",
		"ts-node": "10.9.1",
		"typescript": "4.7.4"
	},
	"engines": {
		"node": "16"
	}
}
